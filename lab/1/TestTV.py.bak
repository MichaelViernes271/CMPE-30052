from TV import TV

# test class for TV class
def main():
    
    tv = TV(20, 2, True)
    tv.turnOn()
    tv.turnOff()
    print(tv.getChannel())
    tv.setChannel(3)
    print(tv.getVolume())
    tv.setVolume(5)
    print(tv.getVolume())
    tv.channelUp()
    tv.channelDown()
    tv.volumeUp()
    tv.volumeDown()
    
if __name__ == "__main__":
    main()